from bs4 import BeautifulSoup as soup
import requests

def simple_get(url):
	res = requests.get(url)
	try:
		res.raise_for_status()
	except Exception as exc:
		print('Error: %s' % (exc))
	else:
		return soup(res.text, 'html.parser')




main_site = 'https://www.bmstores.co.uk/stores/counties/aberdeen'
html_page = simple_get(main_site)
columns = html_page.find_all("th", {"style": "width: 50%;"})
for column in columns:
  for link in column.find_all('a'):
	  print(link.get('href'))
